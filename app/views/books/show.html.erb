<%= render "books/leftpage">
<h1>Books</h1>
<table class = "clearfix">
	<div class = "underbar">
		<tr>
			<th class = "null"></th>
			<th class = "title">Title</th>
	   		<th class = "opinion">Opinion</th>
	   	</tr>
	</div>
   	<% @books.each do |book| %>
	<tr class = "box clearfix">
		<td class = "mypage-icon">
			<a href ="#">
				<%=   %>
			</a>
		</td>
		<td class = "show-title">
			<a href = "edit_user">
				<%= book.title  %>
			</a>
		</td>
	 	<td class = "show-body">
	 		<%= book.body %>
	 	</td>
   	</tr>
	<% end %>
</table>